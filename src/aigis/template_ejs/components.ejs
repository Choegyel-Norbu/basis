<% if(components.length) { %>
<nav class="u-content">
  <h2 class="aigis-module__heading"><%- components[0].config.category[0] %></h2>
  <ul class="aigis-moduleList"><% components.forEach(function(component) { %>
    <li class="aigis-moduleList__item">
      <a href="#<%- component.config.name %>"><%- component.config.name %></a>
    </li><% }) %>
  </ul>
</nav>
<div class="u-content"><% components.forEach(function(component) { %>
  <div class="aigis-module u-content">
    <h2 class="aigis-module__heading" id="<%- component.config.name %>">
      <%- component.config.name %>
    </h2>
    <div class="aigis-module__filepath">
      <%- component.sourcePath %>
    </div>

    <% if (component.config.tag.length) { %>
    <ul class="aigis-tags"><% component.config.tag.forEach(function(tag) { %>
      <li class="aigis-tags__item aigis-tags__item--<%- tag %>">
        <%- tag %>
      </li><% })%>
    </ul>
    <% } %>

    <%- component.html %>
  </div><% }) %>
</div>
<% } else { %>
  <%- html %>
<% } %>
